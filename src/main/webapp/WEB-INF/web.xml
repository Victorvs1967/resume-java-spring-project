<!DOCTYPE xml>

<web-app>

	<display-name>Resume</display-name>
	
	<session-config>
		<tracking-mode>COOKIE</tracking-mode>
	</session-config>

	<servlet>
		<servlet-name>ProfileController</servlet-name>
		<display-name>ProfileController</display-name>
		<description></description>
		<servlet-class>com.vvs.resume.controller.ProfileController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ProfileController</servlet-name>
		<url-pattern>/ProfileController</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>sitemesh</filter-name>
		<filter-class>org.sitemesh.config.ConfigurableSiteMeshFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>sitemesh</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
</web-app>
